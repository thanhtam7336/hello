            end
            end)

            game:GetService('RunService').RenderStepped:connect(function()           
                if _G.chestsea3 then
               if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("God's Chalice") then 
              _G.chestsea3 = false                      
