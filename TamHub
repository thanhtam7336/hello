                                            toTarget(v.HumanoidRootPart.CFrame * CFrame.new(posX,posY,posZ))
                                        until v.Humanoid.Health <= 0 or not v.Parent or not _G.CastleRaid
                                    end
                                end
                            end
                        else
                            toTarget(CFrameCastleRaid)
                        end
                    end)
